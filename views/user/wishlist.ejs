<script>

async function sendCartData(variantId) {
        try {
            const size = 'S';
            console.log(variantId,'variantId');
            
            
           const response=await fetch('/wishlistcart', {
                method: 'POST',
                headers: {
                    'Content-Type': 'application/json'
                },
                body: JSON.stringify({
                    id: variantId,
                    size: size
                })
            })
                const data = await response.json()
                console.log(data, " data");

                if (data.type === 'success') {

                    showSnackBar(data.msg);

            } else if(data.type === 'error') {

                showSnackBar(data.err);
            }

        } catch (err) {
            console.log(err);
        }
    }


    
async function removeItem(variantId, cartId) {  // cartId == itemId
    try {
        const response = await fetch('/remove-wishlist-item', {
            method: 'POST',
            headers: {
                'Content-Type': 'application/json'
            },
            body: JSON.stringify({
                id: variantId,
                cartId: cartId
            })
        });

        if (response.ok) {
            const data = await response.json();
            console.log(data, "item removed");

            // Remove the item row from the DOM
            const itemRow = document.querySelector(`#item-row-${cartId}`);
            itemRow.remove();

            // Update the total cart price in the DOM
            // const totalCartPriceCol = document.querySelector('#total-cart-price');
            // totalCartPriceCol.textContent = `$${data.totalCartPrice.toFixed(2)}`;
        } else {
            console.error('Failed to remove item');
        }
    } catch (error) {
        console.error('Error:', error);
    }
}



function showSnackBar(text) {
  var x = document.getElementById("snackbar");
  x.innerHTML = text
  x.className = "show";
  setTimeout(function(){ x.className = x.className.replace("show", ""); }, 3000);
}

</script>


