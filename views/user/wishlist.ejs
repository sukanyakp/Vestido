									<div class="product">
										<figure class="product-media">
											<a href="#">
												<img src="<%=item.variantDetails.images[0] %>" alt="Product image">
											</a>
                                          
										</figure>

										<h3 class="product-title">
											<a href="#"><%= item.variantDetails.productDetails.productName %></a>
										</h3><!-- End .product-title -->
                                        
									</div><!-- End .product -->
                                  
								</td>
                                <td class="price-col"><%=item.size %></td>
								<td class="price-col">₹<%=item.price %></td>

                                <% if( item.size === 'S'){%>
                                      <% ['S'].forEach((size) => { %>
                                    <% const stock = item.variantDetails.stock.find(s => s.size === size); %>
                                    <% if (stock && stock.quantity > 0) { %>
                                        <td class="stock-col"><span class="in-stock">In stock</span></td>
                                    <% } else { %>
                                        <td class="stock-col"><span class="in-stock">Out of stock</span></td>
                                    <% } %>
                                <% }) %>
                                

                                <% } else if(item.size === 'M' ){%>
                                    <% ['M'].forEach((size) => { %>
                                        <% const stock = item.variantDetails.stock.find(s => s.size === size); %>
                                        <% if (stock && stock.quantity > 0) { %>
                                            <td class="stock-col"><span class="in-stock">In stock</span></td>
                                        <% } else { %>
                                            <td class="stock-col"><span class="in-stock">Out of stock</span></td>
                                        <% } %>
                                    <% }) %>

                                    <% }else if( item.size === 'L'){%>
                                        <% ['L'].forEach((size) => { %>
                                            <% const stock = item.variantDetails.stock.find(s => s.size === size); %>
                                            <% if (stock && stock.quantity > 0) { %>
                                                <td class="stock-col"><span class="in-stock">In stock</span></td>
                                            <% } else { %>
                                                <td class="stock-col"><span class="in-stock">Out of stock</span></td>
                                            <% } %>
                                        <% }) %>



                                        <% } %>



                              
								
								<td class="action-col">
                                   
                                    <div class="dropdown">

                                        <a class="btn-product btn-cart"
                                                    onclick="sendCartData('<%=item.variantDetails._id %>')" id="cart">
                                                    <span>add to cart</span>
                                                    <!-- <span>add to cart</span> -->
                                                </a>
									<!-- <button class="btn btn-block btn-outline-primary-2" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
