<script>




function validateForm() {
        let isValid = true;

        // Name validation
        const current = document.getElementById('currentp').value;
        const currentErr = document.getElementById('currentErr');
        if (current.trim() === "") {
            currentErr.textContent = 'Please enter your password';
            currentErr.style.color = 'red';
            isValid = false;
        } else {
            currentErr.textContent = '';  

        // Password validation
        const password = document.getElementById('newp').value;
        const passwordErr = document.getElementById('pErr');
        if (password.trim() === "") {
            passwordErr.textContent = 'Please enter a new password';
            passwordErr.style.color = 'red';
            isValid = false;
        } else {
            passwordErr.textContent = '';

              // Confirm Password validation
        const confirmPassword = document.getElementById('confirmp').value;
        const confirmPasswordErr = document.getElementById('cErr');
        if (confirmPassword.trim() === "") {
            confirmPasswordErr.textContent = 'Please confirm your password';
            confirmPasswordErr.style.color = 'red';
            isValid = false;
        } else if (password !== confirmPassword) {
            confirmPasswordErr.textContent = 'Passwords do not match';
            confirmPasswordErr.style.color = 'red';
            isValid = false;
        } else {
            confirmPasswordErr.textContent = '';
        }

        }

        }

        return isValid;
    }



const form = document.getElementById('form')
form.addEventListener('submit', (event)=>{
    event.preventDefault();
})

function sendPassword(){
    let currentp = document.getElementById('currentp').value;
    let newp = document.getElementById('newp').value;
    let confirmp = document.getElementById('confirmp').value;

    console.log(currentp, newp, confirmp);

    axios.post(`/password?cIp=${currentp}&nIp=${newp}&coIp=${confirmp}`)
        .then((response) => {
            if(response.status === 200){
                document.getElementById('message').innerHTML = "Password changed successfully";
                // Optionally redirect after a successful password change
                setTimeout(() => {
                    window.location.href = "/profile";
                }, 3000);
               
            }
        })
        .catch((err) => {
            if (err.response) {
                // Server responded with a status other than 2xx
                if(err.response.status === 401){
                    document.getElementById('err').innerHTML = "Entered password is incorrect";
                } else if (err.response.status === 400) {
                    document.getElementById('err').innerHTML = err.response.data;
                } else {
                    document.getElementById('err').innerHTML = "An error occurred";
                }
            } else {
                console.log(err);
                document.getElementById('err').innerHTML = "An error occurred";
            }
        });
}

</script>


