                    <% if (variants && variants.length > 0) { %>
                        <div class="products">
                            <div class="row">
                                <% variants.forEach((v) => { %>
                                    <div class="col-6 col-md-4 col-lg-4 col-xl-3">
                                        <div class="product">
                                            <figure class="product-media">
                                                <span class="product-label label-new">New</span>
                                                <a href="/product?pId=<%= v.products[0]._id %>&vId=<%= v._id %>">
                                                    <img src="<%= v.images[0] %>" alt="Product image" class="product-image">
                                                    <img src="<%= v.images[2] %>" alt="Product image" class="product-image-hover">
                                                </a>
                                                <div class="product-action-vertical">
                                                    <a href="#" class="btn-product btn-wishlist" onclick="sendWishlistData('<%= v._id %>')" title="Wishlist"><span>Add to Wishlist</span></a>
                                                </div>
                                                <div class="product-action action-icon-top">
                                                    <a href="#" class="btn-product btn-cart" onclick="sendCartData('<%= v._id %>')"><span>Add to cart</span></a>
                                                </div>
                                            </figure>
                                            <div class="product-body">
                                                <div class="product-cat">
                                                    <a href="#"><%= v.products[0].productName %></a>
                                                </div>
                                                <h3 class="product-title">
                                                    <a href="/product?pId=<%= v.products[0]._id %>&vId=<%= v._id %>"><%= v.products[0].productName %></a>
                                                </h3>
                                                <div class="product-price">
                                                    <% if (v.offerApplied) { %>
                                                        <span class="new-price">₹<%= v.bestOfferPrice %></span>
                                                        <span class="old-price">₹<%= v.price %></span>
                                                        <span class="offer-label">(<%= v.offer%> %Off)</span>
                                                    <% } else { %>
                                                        ₹<%= v.price %>
                                                    <% } %>
                                                </div>
                                                <div class="ratings-container">
                                                    <p class="mb-0 fw-normal"><%= v.colors %></p>
                                                    <div class="ratings-val" style="width: 0%;"></div>
                                                </div>
                                                <!-- <div class="product-nav product-nav-dots">
                                                    <a href="#" style="background: #cc9966;"><span class="sr-only">Color name</span></a>
                                                    <a href="#" class="active" style="background: #ebebeb;"><span class="sr-only">Color name</span></a>
                                                </div> -->
                                            </div>
                                        </div>
                                    </div>
                                    <p id="category" hidden><%= v.products[0].category %></p>
                                    <p id="brand" hidden><%= v.products[0].brand %></p>
                                <% }) %>
                            </div>
                  
                    
             
                    
                           

                            <div class="pagination-container">
